{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "üêõ GDB Debug (GNU C++)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/${fileBasenameNoExtension}.debug.bin",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "miDebuggerArgs": "-q -x ${workspaceFolder}/.gdbinit",
      "preLaunchTask": "GNU C++ Compile for Debug",
      "logging": {
        "engineLogging": false,
        "trace": false,
        "traceResponse": false
      },
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Disable debuginfod",
          "text": "-gdb-set debuginfod enabled off",
          "ignoreFailures": true
        },
        {
          "description": "Set print array on",
          "text": "-gdb-set print array on",
          "ignoreFailures": true
        },
        {
          "description": "Set print array indexes on",
          "text": "-gdb-set print array-indexes on",
          "ignoreFailures": true
        },
        {
          "description": "Set print elements unlimited",
          "text": "-gdb-set print elements 0",
          "ignoreFailures": true
        },
        {
          "description": "Set disassembly flavor to intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        },
        {
          "description": "Skip all C++ standard library files",
          "text": "-interpreter-exec console \"skip -gfi /usr/include/c++/*\"",
          "ignoreFailures": true
        },
        {
          "description": "Skip all system include files",
          "text": "-interpreter-exec console \"skip -gfi /usr/include/*\"",
          "ignoreFailures": true
        },
        {
          "description": "Skip GCC source debug files",
          "text": "-interpreter-exec console \"skip -gfi /usr/src/debug/*\"",
          "ignoreFailures": true
        },
        {
          "description": "Skip all build directory files",
          "text": "-interpreter-exec console \"skip -gfi */gcc-build/*\"",
          "ignoreFailures": true
        },
        {
          "description": "Skip all libstdc++ files",
          "text": "-interpreter-exec console \"skip -gfi */libstdc++*/*\"",
          "ignoreFailures": true
        }
      ],
      "visualizerFile": "",
      "showDisplayString": true
    },
