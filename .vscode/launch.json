{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Active File",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "preLaunchTask": "g++ build active file",
      "logging": {
        "engineLogging": false,
        "trace": false,
        "traceResponse": false
      },
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        },
        {
          "description": "Disable debuginfod",
          "text": "-gdb-set debuginfod enabled off",
          "ignoreFailures": true
        },
        {
          "description": "Set print array on",
          "text": "-gdb-set print array on",
          "ignoreFailures": true
        },
        {
          "description": "Set print array indexes on",
          "text": "-gdb-set print array-indexes on",
          "ignoreFailures": true
        }
      ],
      "visualizerFile": "",
      "showDisplayString": true
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/yehj/repos/algo/prac",
      "program": "/home/yehj/repos/algo/prac/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}